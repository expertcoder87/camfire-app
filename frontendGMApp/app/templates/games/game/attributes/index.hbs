  <div class="layout-row flex">
    <div class= "flex-80 sc-title"> ATTRIBUTES </div>
    {{#paper-button primary=true class="flex-20" onClick=(action "createAttribute")}} Create Attribute {{/paper-button}}
  </div>
  {{#paper-list}}
    {{#each model as |attribute|}}
      {{#paper-item onClick=(action "openAttribute" attribute) class="coolPaperItem"}}
        <div class="layout-column flex">
          <div class="flex-50">
            <div class="layout-row flex">
                <h3 class="flex-90">{{attribute.name}}</h3>
            </div>
          </div>
          <div class="flex-50">
            <p>
              {{attribute.description}}
            </p>
          </div>
        </div>


      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
