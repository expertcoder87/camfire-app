  <div class="layout-row flex">
    <div class= "flex-85 sc-title"> SKILLS </div>
    {{#paper-button primary=true class="flex-15" onClick=(action "createSkill")}} Create Skill {{/paper-button}}
  </div>
  {{#paper-list}}
    {{#each model as |skill|}}
      {{#paper-item onClick=(action "openSkill" skill) class="coolPaperItem"}}
        <div class="layout-column flex">
          <div class="flex-50">
                <hgroup>
                <h3>
                  {{skill.name}}
                  <p class=text-byline> {{skill.context.name}} </p>
                </h3>
              </hgroup>
          </div>

          <div class="flex-50">
            <p>
              {{skill.description}}
            </p>
          </div>
      </div>


      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
